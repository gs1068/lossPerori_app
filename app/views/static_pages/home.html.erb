          <div class="top-catch-area">
            <h1>食品ロスのゼロを目指す。</h1>
            <div class="header-right-nav">
              <% if user_signed_in? %>
                <p>検索欄</p>
              <% else %>
                <%= link_to "会員登録", new_user_registration_path, class: "btn btn-first btn-appearance" %>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="new_information">
    <p>新着情報</p>
    <div class="row">
      <%= render 'home_product_content', products: @products %>
    </div>
  </div>
</div>
